    "# CALCULATIONS OF PERFORMANS METRICS\n",
    "\n",
    "def TP(testingLabels, predictedLabels):\n",
    "    tp=0\n",
    "    for i in personType:\n",
    "        for j in range(len(testingLabels)):\n",
    "            if testingLabels[j] == i and predictedLabels[j] == i:\n",
    "                tp+=1\n",
    "\n",
    "    return tp\n",
    "\n",
    "def TN(testingLabels, predictedLabels):\n",
    "    tn=0\n",
    "    for i in personType:\n",
    "        for j in range(len(testingLabels)):\n",
    "            if testingLabels[j] != i and predictedLabels[j] != i:\n",
    "                tn+=1\n",
    "\n",
    "    return tn\n",
    "\n",
    "def FN(testingLabels, predictedLabels):\n",
    "    fn=0\n",
    "    for i in personType:\n",
    "        for j in range(len(testingLabels)):\n",
    "            if testingLabels[j] == i and predictedLabels[j] != i:\n",
    "                fn+=1\n",
    "\n",
    "    return fn\n",
    "\n",
    "def FP(testingLabels, predictedLabels):\n",
    "    fp=0\n",
    "    for i in personType:\n",
    "        for j in range(len(testingLabels)):\n",
    "            if testingLabels[j] != i and predictedLabels[j] == i:\n",
    "                fp+=1\n",
    "\n",
    "    return fp\n",
    "\n",
    "def Accuracy(testingLabels, predictedLabels):\n",
    "    tp = TP(testingLabels, predictedLabels)\n",
    "    fp = FP(testingLabels, predictedLabels)\n",
    "    fn = FN(testingLabels, predictedLabels)\n",
    "    tn = TN(testingLabels, predictedLabels)\n",
    "    acc_score = (tp + tn) / (tp + tn + fp + fn)\n",
    "    return acc_score\n",
    "\n",
    "def Precision(testingLabels, predictedLabels):\n",
    "    tp = TP(testingLabels, predictedLabels)\n",
    "    fp = FP(testingLabels, predictedLabels)\n",
    "    precision = tp/ (tp + fp)\n",
    "    return precision\n",
    "\n",
    "def Recall(testingLabels, predictedLabels):\n",
    "    tp = TP(testingLabels, predictedLabels)\n",
    "    fn = FN(testingLabels, predictedLabels)\n",
    "    recall = tp/ (tp + fn)\n",
    "    return recall"
   ]
